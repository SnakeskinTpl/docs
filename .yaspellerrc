{
  "lang": "en,ru",

  "fileExtensions": [
    ".html",
    ".md"
  ],

  "excludeFiles": [
    ".git",
    "node_modules",
    "styles",
    "css",
    "scripts",
    "js",
    "tpls"
  ],

  "dictionary": [
    "/\\b(snakeskin|nakeskin|snakeskinTpl|ss|v7|filename|dirname)\\b/i",
    "/\\b(es2015|ECMAScript|js|CoffeeScript|github|io|com|filename|dirname)\\b/i",
    "/\\b(html5|dtd|mathml|svg|bem|frameset)\\b/i",
    "/\\b(mvvm|vue|xss|webpack|foo)\\b/i",
    "/\\b(importFilters|setFilterParams|undef)\\b/i",
    "/\\b(attr|doctype|forEach|forIn|putIn)\\b/i",
    "/бесшовно|биндинг|бэкенд|фронтенд|доопределение/i",
    "/плейсходеры?/i",
    "/реюза?/i",
    "/ништяки?/i",
    "/шаблонизаци(и|я)/i",
    "/фреймворк(и|ов)?/i",
    "/плейсхолдеры?/i",
    "/инлайн(овые|овым|овых)?/i",
    "/runner.ами/i",
    "ые"
  ]
}
