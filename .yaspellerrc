{
  "lang": "en,ru",

  "fileExtensions": [
    ".html",
    ".md"
  ],

  "excludeFiles": [
    ".git",
    "node_modules",
    "styles",
    "css",
    "scripts",
    "js",
    "tpls"
  ],

  "dictionary": [
    "/\\b(snakeskin|nakeskin|snakeskinTpl|ss|v7|filename|dirname)\\b/i",
    "/\\b(es2015|ECMAScript|js|CoffeeScript|github|io|com|filename|dirname|codepen)\\b/i",
    "/\\b(html5|xhtml|textarea|haml|meta|jade|dtd|mathml|svg|bem|frameset|acss|cljs|ls|ts)\\b/i",
    "/\\b(mvvm|npm|webkit|vue|xss|webpack|foo|cli|std|jsdoc|myData|gulpfile|camelCase)\\b/i",
    "/\\b(importFilters|setFilterParams|undef|tolerateWhitespaces|i18nFn|i18nFnOptions|inlineTags)\\b/i",
    "/\\b(bemFilter|literalBounds|renderAs|renderMode|async)\\b/i",
    "/\\b(attr|doctype|forEach|forIn|putIn|cdata)\\b/i",
    "/бесшовно|биндинг|бэкенд|фронтенд|доопределение|фич(а|и)?|кейз/i",
    "/шаблонизатор(а|ы)?/i",
    "/Фреймворк(и|ами)?/i",
    "/микробиблиотека/i",
    "/отрендерит(ся)?|скомпилирует(ся)?/i",
    "/полифил(ов|ы)?/i",
    "/плейсходеры?/i",
    "/реюза?/i",
    "/лоадеры?/i",
    "/ништяки?/i",
    "/шаблонизаци(и|я)/i",
    "/фреймворк(и|ов)?/i",
    "/плейсхолдеры?/i",
    "/инлайн(овые|овым|овых|овая)?/i",
    "/runner.ами/i",
    "ые"
  ]
}
